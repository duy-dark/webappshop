  <div id="dangNhapTitle">
      ĐĂNG NHẬP
  </div>
<div id="dangNhapBackground">
<div id="dangNhapInform">
    
</div>
<div id="dangNhapContent">
<form class=" form-horizontal" data-toggle="validator" role="form" method="post" >
    <br>
    <div class="form-group">
        <label for="userName" class="col-xs-3 col-sm-3 col-md-3 col-lg-3 control-label">User Name:</label>
        <div class="col-xs-8 col-sm-8 col-md-8 col-lg-8 ">
            <input type="text" class="form-control" id="userName" name="fuserName" placeholder="Nhập username" data-error="Vui lòng nhập tên đăng nhập" required>
            <p class="help-block with-errors"> </p>
        </div>
    </div>
    <div class="form-group">
        <label for="password" class="col-xs-3 col-sm-3 col-md-3 col-lg-3 control-label">Password:</label>
        <div class="col-xs-8 col-sm-8 col-md-8 col-lg-8 ">
            <input type="password" class="form-control" id="password" name="fpassword" placeholder="Nhập password" data-error="Vui lòng nhập password" required>
            <p class="help-block with-errors"> </p>
        </div>
    </div>
    <span class="col-xs-9 col-sm-9 col-md-9 col-lg-9"></span>
    <button class="btn-success font" id="btnLogin">Login</button>
</form>
</div>
</div>